technique [
    name = "effect"
    ps_main="psmain"
    gs_main="gsmain"
    vs_main="vsmain"
           ];


exchange struct VSOutput {
  [semantic="position" system="true"]
  vec3 position;
  vec3 normal;
  vec2 texcoord;
};

exchange struct VSInput {
  [semantic="position"]
  vec3 position;
  vec3 normal;
  vec3 texcoord;
};

VSOutput vsmain(VSInput input) {
  VSOutput o;
  o.position = input.position;
  o.normal = input.normal;
  o.texcoord = input.texcoord;
  return o;
}

[max_vertex_count="18" vertex_type="VSOutput" primitive_type="line"]
void gsmain(VSOutput v) {
  gsemit VSOutput nv;
  emit_vertex();
  end_primitive();
}

[system="true" semantic="target"]
vec4 psmain(VSOutput o) {
  return vec4(1, 1, 1, 1);
}
